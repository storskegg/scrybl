{
  "name": "scrybl",
  "version": "0.0.1",
  "description": "Browser console override implementing Beacon API to push logging to a known endpoint.",
  "main": "lib/scrybl.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js",
    "cypress": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storskegg/scrybl.git"
  },
  "keywords": [],
  "author": "Liam Conrad <liam@storskegg.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storskegg/scrybl/issues"
  },
  "homepage": "https://github.com/storskegg/scrybl#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-cypress": "^2.0.1",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "cypress": "^2.1.0"
  }
}
