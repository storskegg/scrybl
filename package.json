{
  "name": "scrybl",
  "version": "2.1.0",
  "description": "Browser console override implementing Beacon API to push logging to a known endpoint.",
  "main": "lib/scrybl.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require @babel/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./test/*.spec.js",
    "cypress": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storskegg/scrybl.git"
  },
  "keywords": [],
  "author": "Liam Conrad <liam@storskegg.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storskegg/scrybl/issues"
  },
  "homepage": "https://github.com/storskegg/scrybl#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-uglify": "^1.0.2",
    "chai": "^4.3.6",
    "cypress": "^10.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha": "^10.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "core-js": "^3.22.8"
  }
}
